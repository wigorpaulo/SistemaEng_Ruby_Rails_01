<p id="notice"><%= notice %></p>

<h1>Tipo Enderecos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_enderecos.each do |tipo_endereco| %>
      <tr>
        <td><%= tipo_endereco.descricao %></td>
        <td><%= tipo_endereco.status %></td>
        <td><%= link_to 'Show', tipo_endereco %></td>
        <td><%= link_to 'Edit', edit_tipo_endereco_path(tipo_endereco) %></td>
        <td><%= link_to 'Destroy', tipo_endereco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Endereco', new_tipo_endereco_path %>
