<p id="notice"><%= notice %></p>

<h1>Fornecedor Contatos</h1>

<table>
  <thead>
    <tr>
      <th>Id fornecedor</th>
      <th>Telefone</th>
      <th>Celular</th>
      <th>Num whats zap</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fornecedor_contatos.each do |fornecedor_contato| %>
      <tr>
        <td><%= fornecedor_contato.id_fornecedor %></td>
        <td><%= fornecedor_contato.telefone %></td>
        <td><%= fornecedor_contato.celular %></td>
        <td><%= fornecedor_contato.num_whats_zap %></td>
        <td><%= fornecedor_contato.email %></td>
        <td><%= link_to 'Show', fornecedor_contato %></td>
        <td><%= link_to 'Edit', edit_fornecedor_contato_path(fornecedor_contato) %></td>
        <td><%= link_to 'Destroy', fornecedor_contato, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fornecedor Contato', new_fornecedor_contato_path %>
